{
  "projectSettings": {
    "framework": "nextjs"
  },
  "buildCommand": "cd ../.. && pnpm build --filter=web",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && pnpm install",
  "functions": {
    "app/api/**": {
      "maxDuration": 60
    }
  },
  "env": {
    "NEXT_PUBLIC_APP_URL": {
      "@type": "system",
      "@value": "VERCEL_URL"
    }
  },
  "regions": ["iad1"]
}