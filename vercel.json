{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "pnpm install",
        "buildCommand": "cd apps/web && pnpm build"
      }
    }
  ],
  "functions": {
    "apps/web/app/api/**": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ]
}